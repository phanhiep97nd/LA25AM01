body {
    /* padding: 0;
    margin: 0; */
    box-sizing: border-box;
}

.logo {
    width: 35%;
}

.logo .img {
    border-right: 1px solid;
}

.logo h2 {
    text-align: center;
}

.clearfix::after {
    content: "";
    display: block;
    clear: both;
}

.banner>table {
    float: left;
}

.banner>a {
    float: left;
    align-items: center;
    padding-left: 10px;
    margin-top: 61px;
}

.content table {
    width: 100%;
    text-align: center;
}

.content table th {
    background-color: rgba(0, 0, 0, 0.2);
}


